@model IEnumerable<SurveyForm.Models.DbEntities.People>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
<table class="table">
    <thead>
        <tr>
            @* <th>Id</th> *@
            <th>Name</th>
            <th>Email</th>
            <th>ContactNo</th>
            <th>Area</th>
            <th>State</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                @* <td>@item.Id</td> *@
                <td>@item.Name</td>
                <td>@item.EmailId</td>
                <td>@item.ContactNo</td>
                <td>@item.Area</td>
                <td>@item.HouseNo</td>
                <td>@item.State</td>
                <td>@item.City</td>
            </tr>
        }

    </tbody>
</table>